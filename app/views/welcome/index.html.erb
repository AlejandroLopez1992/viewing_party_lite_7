<% if current_user %>
  <%= button_to "Log Out", "/logout", method: :get %>
<% else%>
  <%= button_to "Create New User", new_user_path, method: :get %>
  <%= button_to "Log In", "/login", method: :get %>
<% end%>

<% if current_user %>
  <h3>Users:</h3>
  <div id='users'>
  <% @users.each do |user| %>
  <li><%= user.email %></li>
  <% end %>
  </div>
<% end %>

<p>
  The time is now: <%= Time.now %>
</p>